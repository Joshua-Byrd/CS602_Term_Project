<form method="post" action="/admin/user/edit" class="form-group">

  <p><label for="firstName">First Name:</label>
    <input
      type="text"
      id="firstName"
      name="firstName"
      value="{{data.firstName}}"
    /></p>

  <p><label for="lastName">Last Name:</label>
    <input
      type="text"
      id="lastName"
      name="lastName"
      value="{{data.lastName}}"
    /></p>

  <p><label for="email">Email:</label>
    <input type="text" id="email" name="email" value="{{data.email}}" /></p>
  <p><label for"admin">AdminL</label>
    <input type="text" id="admin" name="admin" value="{{data.admin}}"></p>  

  <p><label for="bio">Bio:</label>
    <input type="text" id="bio" name="bio" value="{{data.bio}}" /></p>

  <input type="hidden" id="id" name="id" value="{{data.id}}" />
  <input type="submit" />
</form>
<nav class="bottom-nav">
<a href="/admin/users">Go back</a>
</nav>